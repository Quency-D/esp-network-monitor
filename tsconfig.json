{
	"compilerOptions": {
	  "module": "Node16",
	  "moduleResolution": "node16",
	  "target": "ES2020",
	  "outDir": "out",
	  "lib": ["ES2020"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "skipLibCheck": true,
	  "strict": false,
	  "strictNullChecks": true,
	  "noImplicitOverride": true,
	  "resolveJsonModule": true
	}
  }
  